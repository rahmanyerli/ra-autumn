			"scope": "storage",
			"settings": {
				"foreground": "#cc8833"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#cc8833"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#cc8833"
			}
		},
		{
			"scope": [
				"string",
				"entity.name.operator.custom-literal.string"
			],
			"settings": {
				"foreground": "#009933"
			}
		},
		{
			"scope": [
				"string.comment.buffered.block.pug",
				"string.quoted.pug",
				"string.interpolated.pug",
